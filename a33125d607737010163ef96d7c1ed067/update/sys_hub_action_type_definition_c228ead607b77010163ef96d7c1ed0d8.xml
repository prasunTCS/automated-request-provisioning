        <value>(function execute(inputs, outputs) {
// ... code ...
 
  //Lookup record in Automation candidate matrix table
  var recordMatch = 'False';
  var acChecker = new GlideRecord('u_automation_qualifier_check_matrix');
  acChecker.addQuery('u_catalog_item',inputs.catalog_item_sys_id);
  acChecker.query()
  while(acChecker.next())
  {
      recordMatch = checkRecordMatches(acChecker);
  }
   
  //returns the variable name 
  function checkRecordMatches(recordObject){
  
    var variableMtoM = new GlideRecord('sc_item_option_mtom');
    variableMtoM.addEncodedQuery('request_item='+inputs.ritm_sys_id+'^'+recordObject.u_catalog_qualifier_encoded_query.toString());
    variableMtoM.query();
    while(variableMtoM.next())
    {
      gs.log('Found');
      return 'True';
    }
    
    
    
  
  outputs.candidate_found = recordMatch;
  
}
  
})(inputs, outputs);

</value>
